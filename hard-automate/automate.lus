include "memoire.lus"

-- Trouvé à partir d'un tableau de kernaugh
function transition (q1, q2, e : bool)
returns (nq1, nq2, s : bool);
let
    nq1 = not (q1 xor e);
    nq2 = q2 xor e;
    s = nq1 and nq2;
tel;

node automate (car : bool)
returns (ec1, ec2, s : bool);
let
    ec1, ec2, s = transition(true -> pre ec1, true -> pre ec2, car);
tel;
